---
import SectionHeaderSvelte from './SectionHeader.svelte';

export interface Props {
  title: string;
  showLine?: boolean;
  count?: string;
}

const { title, showLine = true, count } = Astro.props;
---

<SectionHeaderSvelte 
  client:visible
  title={title}
  showLine={showLine}
  count={count}
/>